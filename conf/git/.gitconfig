[color]
  ui = auto

[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green

[color "diff"]
  meta = cyan bold
  frag = yellow
  old = red
  new = green

[color "status"]
  added = green
  changed = red
  untracked = cyan

[core]
  whitespace = fix,-indent-with-non-tab,-indent-with-tab,trailing-space,cr-at-eol
  editor = vim

[push]
  default = tracking

[alias]
  st = status
  ci = commit
  br = branch
  co = checkout
  df = diff
  dfs = diff --cached
  w = whatchanged
  lg = log --graph --decorate --pretty=oneline --abbrev-commit
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p
  